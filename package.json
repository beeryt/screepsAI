{
  "name": "screepsAI",
  "description": "An in-development bot for screeps.com",
  "version": "0.0.1",
  "author": "Carl Beery (https://github.com/beeryt)",
  "license": "Unlicensed",
  "scripts": {
    "clean": "del-cli .nyc_output tsc-out dist",
    "build": "tsc",
    "test": "nyc ava",
    "lint": "eslint src --ext .js,.ts",
    "docs": "typedoc --excludePrivate --out docs src",
    "deploy": "rollup -c --environment DEST:main"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/structures/**/*.ts"
    ],
    "exclude": ["src/main.ts"],
    "sourceMap": true,
    "require": [
      "esm",
      "ts-node/register",
      "lodash"
    ],
    "all": true
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/lodash": "^3.10.1",
    "@types/screeps": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@tyriar/avl-tree": "^2.0.6",
    "ava": "^1.4.1",
    "del-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "mkdirp": "^0.5.1",
    "node": "^10.15.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rollup": "^1.11.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-node": "^8.1.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@tyriar/fibonacci-heap": "^2.0.9"
  }
}
