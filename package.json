{
  "name": "screepsAI",
  "description": "An in development bot for screeps.com",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf tsc-out && rm -rf dist",
    "lint": "eslint src --ext .js,.ts",
    "build": "tsc -p .",
    "test": "ava tests",
    "docs": "typedoc --excludePrivate --out docs src",
    "start": "node tsc-out/main.js",
    "deploy": "rollup -c --environment DEST:main"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "test/**/*"
    ],
    "sources": [
      "src/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ],
    "timeout": "5s"
  },
  "devDependencies": {
    "@types/lodash": "^3.10.1",
    "@types/screeps": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@tyriar/avl-tree": "^2.0.6",
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "node": "^10.15.3",
    "rollup": "^1.11.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-node": "^8.1.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@tyriar/fibonacci-heap": "^2.0.9"
  }
}
